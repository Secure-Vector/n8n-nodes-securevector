{
  "name": "n8n-nodes-securevector",
  "version": "0.1.0",
  "description": "SecureVector AI security scanning for n8n workflows - detect prompt injection, jailbreak_attempts, malicious content and more.. See LICENSE.",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "securevector",
    "security",
    "ai-security",
    "llm-security",
    "prompt-injection",
    "threat-detection"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/Secure-Vector/n8n-nodes-securevector#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Secure-Vector/n8n-nodes-securevector.git"
  },
  "bugs": {
    "url": "https://github.com/Secure-Vector/n8n-nodes-securevector/issues"
  },
  "author": {
    "name": "SecureVector",
    "email": "support@securevector.io"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && node scripts/post-build.js",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "prepack": "npm run build",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SecureVectorApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SecureVector/SecureVector.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-n8n-nodes-base": "^1.16.4",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^2.2.1"
  },
  "dependencies": {
    "zod": "^3.25.76"
  }
}
